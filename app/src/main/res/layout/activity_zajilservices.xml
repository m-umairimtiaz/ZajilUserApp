<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ELibrary">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="10dp"
        android:background="@drawable/galaxy2"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Connectivity Services"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:textSize="24sp"
            android:padding="5dp"
            android:textColor="#008000"
            android:textStyle="bold"
            ></TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="The Building Blocks of the New industrial revolution"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:textSize="12sp"
            android:padding="5dp"
            android:textColor="@color/black"
            android:textStyle="bold"
            ></TextView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_margin="5dp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp"
                android:id="@+id/uploadNotice">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/datadenter"
                        android:padding="0dp"/>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="0dp"/>


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/aclonica"
                        android:text="Data Center"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp"
                android:id="@+id/uploadImage">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/widenetwork"
                        android:padding="0dp"/>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="0dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="SD-WAN"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="10dp"
                        android:textColor="@color/textColor" />

                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_margin="5dp">


            <com.google.android.material.card.MaterialCardView
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp"
                android:id="@+id/addEbook">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="95dp"

                        android:src="@drawable/vc"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="5dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Voice"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/textColor" />

                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                android:id="@+id/faculty"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/wifi"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="5dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="50dp"
                        android:text="Wireless Service"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="5dp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/textColor" />

                </LinearLayout>

            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Security Services"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:textSize="24sp"
            android:padding="5dp"
            android:textColor="#008000"
            android:textStyle="bold"
            ></TextView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_margin="5dp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp"
                android:id="@+id/news">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/ft"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="5dp"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Fortigate - Palo Alto Firewalls"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="0dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                android:id="@+id/student"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/ds"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="5dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="60dp"
                        android:text="DDOS Protection"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="5dp"
                        android:layout_marginTop="5dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cloud Services"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:textSize="24sp"
            android:padding="5dp"
            android:textColor="#008000"
            android:textStyle="bold"
            ></TextView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_margin="5dp">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/delete"
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/microsoftzzure"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="0dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Microsoft Azure Services"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="0dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/logout"
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/ms365"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="0dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Microsoft Office 365"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="1dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_margin="5dp">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/ddos"
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="95dp"

                        android:src="@drawable/ds"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="0dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="BAAS (Backup as a Service)"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="3dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/iaas"
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/iaas"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="0dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="60dp"
                        android:text="IAAS (Infrastructure Service)"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="0dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cyber Security Services"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:textSize="24sp"
            android:padding="5dp"
            android:textColor="#008000"
            android:textStyle="bold"
            ></TextView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_margin="5dp">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/baas"
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/cyberdefence"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="5dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="50dp"
                        android:text="Zajil Cyber Defense Center"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="0dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/network"
                android:layout_width="180dp"
                android:layout_height="170dp"
                android:layout_margin="10dp"
                app:cardElevation="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"

                        android:src="@drawable/networksecurity"
                        android:padding="0dp"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="3dp"
                        android:background="@color/lightGray"
                        android:layout_marginTop="5dp"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Network and Infrastructure Security"
                        android:fontFamily="@font/aclonica"
                        android:textStyle="bold"
                        android:padding="0dp"
                        android:layout_marginTop="0dp"
                        android:textColor="@color/textColor" />



                </LinearLayout>


            </com.google.android.material.card.MaterialCardView>

        </LinearLayout>
    </LinearLayout>



</ScrollView>